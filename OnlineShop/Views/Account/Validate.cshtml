
@{
    ViewData["Title"] = "Validate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (ViewData["validate"] != null)
{
    <h1 class="text-center text-primary mt-3"><i class="fas fa-laugh-beam"></i></h1>
    <h1 class="m-5 text-center">GRACIAS POR CONFIRMAR SU CUENTA YA PUEDE ENTRAR, ANIMATE Y COMPRA ALGUNOS DE NUESTROS PRODUCTOS</h1>
}
else
{
    <h1 class="text-center text-primary mt-3"><i class="fas fa-sad-cry"></i></h1>
    <h1 class="m-5 text-center">Lo sentimos este codigo ya no es valido</h1>
}

